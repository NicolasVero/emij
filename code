if (! defined(‘ABSPATH’)) {
    exit;
}
get_header();
    if( is_post_type_archive( ‘artistes’ ) ){
        get_template_part( ‘template-parts/artistes/content’, ‘artistes’ );
    }elseif( is_post_type_archive( ‘agenda’ ) ){
        get_template_part(‘template-parts/agenda/content’, ‘list-dates’);
    }elseif( is_post_type_archive( ‘actualites’ ) ){
        get_template_part(‘template-parts/actualites/content’, ‘page-actualites’);
    }else{
        get_template_part(‘template-parts/content’, ‘page’);
    }
get_footer();?>